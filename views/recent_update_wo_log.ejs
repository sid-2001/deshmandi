
<section id="jobs">

<div class="container">
    
    <h1 class="text-center"><strong>Recent Update's</strong></h1>

    <% var len=raye.length%>
    <%var i%>
  
    <%for(i=(len-1);i>=0;i--){%>
         <div class="company-detail">
       
    <div class="job-update">
        <h4><b><%=raye[i].category%></b></h4>
        <p> विक्रेता 
नाम - <%=raye[i].name%></p>
        <i class="fab fa-envira"></i>
        <span>फसल की उम्र -<%=raye[i].cropage%>  
दिन</span><br>
        <i class="fa fa-inr"></i><span><%=raye[i].cropcost%></span>
        <br>
        <i class="fa fa-map-marker"></i><span><%=raye[i].city%></span>
       <br>
        <i class="fa fa-weight-hanging"></i><span><%=raye[i].weight%> 
क्विंटल</span>
        </div>
        <div class="apply-btn">
            
                 <button type="submit" class="btn btn-primary" onclick="changloc();">
            प्रस्ताव भेजें</button>


            <script>
            
            function changloc(){
                
     location.replace("/signin");
                
            }
            
            
            </script>
        </div>
    </div>
        

   <%}%>
    
    
    
     
    </div>



</section>